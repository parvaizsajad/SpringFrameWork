package Spring.LifeCycle;

public class Somosa {
private double somasa;

public Somosa(double somasa) {
	super();
	this.somasa = somasa;
}

public double getSomasa() {
	return somasa;
}

public void setSomasa(double somasa) {
	this.somasa = somasa;
}

@Override
public String toString() {
	return "Somosa [somasa=" + somasa + "]";
}
public void init() {
	System.out.println("this is the init method");
}
public void destroy() {
	System.out.println("this is the destroy method");
}
}
